<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<RepositoryUrl>https://github.com/WonderStudioOfficial/Refit.Attributes</RepositoryUrl>
		<Title>Wonder Refit Attributes</Title>
		<PackageTags>wonder;refit;attribute;post;get;patch;delete;put</PackageTags>
		<VersionPrefix>8.0.0</VersionPrefix>
		<VersionSuffix>alpha</VersionSuffix>
		<Authors>Wonder Studio</Authors>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Refit" />
	</ItemGroup>

</Project>
